<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net48;netstandard2.1;netcoreapp3.1;net6.0-windows</TargetFrameworks>
		<Platforms>x64</Platforms>
		<OutputType>Library</OutputType>
	</PropertyGroup>

	<PropertyGroup>
		<Title>ExtendedArithmetic.BigComplex</Title>
		<Product>ExtendedArithmetic.BigComplex</Product>
		<AssemblyName>ExtendedArithmetic.BigComplex</AssemblyName>
		<AssemblyTitle>ExtendedArithmetic.BigComplex</AssemblyTitle>
		<PackageId>ExtendedArithmetic.BigComplex</PackageId>
		<Description>Arbitrary precision complex (imaginary) numbers.</Description>
		<Authors>Adam White</Authors>
		<Company>Adam White</Company>
		<Copyright>Made by Adam White</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>math mathematics biginteger complex-numbers arbitrary-precision numerics arithmetic numbers charp</PackageTags>
		<PackageProjectUrl>https://www.nuget.org/packages/ExtendedNumerics.BigComplex</PackageProjectUrl>
		<RepositoryUrl>https://github.com/AdamWhiteHat/BigComplex</RepositoryUrl>
		<RepositoryType>git</RepositoryType>

		<Version>$([System.DateTime]::Now.ToString("yyyy."$([System.DateTime]::Now.DayOfYear)".HHmm"))</Version>
		<AssemblyVersion>$([System.DateTime]::Now.ToString("yyyy."$([System.DateTime]::Now.DayOfYear)".HHmm"))</AssemblyVersion>
		<FileVersion>$([System.DateTime]::Now.ToString("yyyy."$([System.DateTime]::Now.DayOfYear)".HHmm"))</FileVersion>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="ExtendedNumerics.BigDecimal" Version="2022.187.2158" />
	</ItemGroup>

</Project>